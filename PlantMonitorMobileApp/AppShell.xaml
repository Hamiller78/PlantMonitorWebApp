<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PlantMonitorMobileApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PlantMonitorMobileApp"
    xmlns:pages="clr-namespace:PlantMonitorMobileApp.Pages">

    <FlyoutItem Title="Overview" >
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate pages:PlantOverviewPage}" />
        </Tab>
    </FlyoutItem>
    <FlyoutItem Title="Detail" >
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate pages:PlantDetailPage}" />
        </Tab>
    </FlyoutItem>
</Shell>
